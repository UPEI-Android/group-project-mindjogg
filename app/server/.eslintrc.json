{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "prefer-const": "error",
        "quotes": "error",
        "no-inline-comments": "error",
        "semi-style": "error",
        "require-await": "error"
    }
}
